{
	"name": "prepsilon",
	"version": "1.0.0",
	"main": "backend/build/core.js",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/DarkGuy10/Prepsilon.git"
	},
	"author": "DarkGuy10 <HarperBurn@protonmail.ch>",
	"license": "MIT",
	"workspaces": [
		"frontend",
		"backend"
	],
	"scripts": {
		"frontend:dev": "next dev frontend",
		"frontend:build": "next build frontend",
		"frontend:start": "next start frontend",
		"frontend:lint": "next lint frontend",
		"backend:build": "tsc -p backend",
		"backend:lint": "eslint backend",
		"lint": "prettier -w . && yarn backend:lint && yarn frontend:lint",
		"boot": "yarn backend:build && electron .",
		"start": "concurrently -k \"yarn frontend:dev\" \"wait-on tcp:127.0.0.1:3000 && yarn boot\"",
		"pre-build": "yarn frontend:build && yarn backend:build",
		"build": "yarn pre-build && electron-builder",
		"postinstall": "electron-builder install-app-deps && husky install"
	},
	"packageManager": "yarn@3.6.0",
	"dependencies": {
		"discord.js": "^14.11.0",
		"electron-store": "^8.1.0",
		"electron-updater": "^5.3.0",
		"next": "13.4.4",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "20.2.5",
		"@types/react": "18.2.8",
		"@types/react-dom": "18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"concurrently": "^8.1.0",
		"electron": "^25.0.1",
		"electron-builder": "^23.6.0",
		"eslint": "^8.42.0",
		"eslint-config-next": "13.4.4",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3",
		"wait-on": "^7.0.1",
		"husky": "^8.0.0"
	}
}
